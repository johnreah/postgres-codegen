<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC
        "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

    <table schema="public" name="customer" class="com.johnreah.accounts.springdatajpa.generated.CustomerEntity">
        <meta attribute="class-description">
            Javadoc for the auto-generated Customer class
            inserted by the hibernate.reveng.xml config file
        </meta>
        <primary-key>
            <generator class="identity"></generator>
        </primary-key>
    </table>
    
    <table schema="public" name="account_type" class="com.johnreah.accounts.springdatajpa.generated.AccountTypeEntity">
        <primary-key>
            <generator class="identity"></generator>
        </primary-key>
    </table>

    <table schema="public" name="account_history" class="com.johnreah.accounts.springdatajpa.generated.AccountHistoryEntity">
        <primary-key>
            <generator class="identity"></generator>
        </primary-key>
    </table>

    <table schema="public" name="account" class="com.johnreah.accounts.springdatajpa.generated.AccountEntity">
        <primary-key>
            <generator class="identity"></generator>
        </primary-key>
    </table>

</hibernate-reverse-engineering>
